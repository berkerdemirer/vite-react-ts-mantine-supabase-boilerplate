{
  "name": "vite-react-supabase-boilerplate",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "downloadEn": "locize download --project-id=596dc0ad-dac9-48af-8088-6413160596b4 --language=en --ver=latest --clean=true --path=./src/@types/locales",
    "interface": "i18next-resources-for-ts interface -i ./src/@types/locales -o ./src/@types/resources.d.ts",
    "update-interface": "npm run downloadEn && npm run interface && rm -rf ./src/@types/locales"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@mantine/core": "^7.5.3",
    "@mantine/dates": "^7.5.3",
    "@mantine/dropzone": "^7.5.3",
    "@mantine/hooks": "^7.5.3",
    "@mantine/modals": "^7.5.3",
    "@mantine/notifications": "^7.5.3",
    "@reduxjs/toolkit": "^2.2.1",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.39.6",
    "@tabler/icons-react": "^2.47.0",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "i18next": "^23.8.3",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-locize-backend": "^6.4.1",
    "locize-lastused": "^3.4.0",
    "react": "^18.2.0",
    "react-circle-flags": "^0.0.20",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-i18next": "^14.0.5",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/node": "20.11.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "happy-dom": "^13.3.8",
    "i18next-resources-for-ts": "1.5.0",
    "locize-cli": "8.0.0",
    "postcss": "^8.4.35",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "3.2.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vitest": "^1.3.0"
  }
}
